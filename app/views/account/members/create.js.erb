<% if @shared_workspace.persisted? %>
  $('#workspace-members')
  .append("<%= j render 'account/members/show_user', members: [@shared_workspace.user], workspace: @workspace %>")
  $('#new-member').html("<%= j render 'account/members/new_link', workspace: @workspace %>")
<% else %>
  $('#new-member').html("<%= j render 'account/members/form', workspace: @workspace, shared_workspace: @shared_workspace %>");
<% end %>
