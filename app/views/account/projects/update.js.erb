<% if @result %>
  $('.modal').removeClass('is-active')
  $('[data-id = "<%= @project.id %>"]').
  replaceWith("<%= j render 'account/projects/show_project', project: @project, workspace: @workspace%>");
<% else %>
  $('.modal-content').html("<%= j render 'account/projects/form', project: @project, workspace: @workspace %> ");
<% end %>
