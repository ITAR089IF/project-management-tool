$('#incomplete_tasks').html('');
<%- @incomplete_tasks.each do |task| %>
var task_html = $("<%= j render('/account/projects/show_tasks', task: task, project: @project) %>");
$('#incomplete_tasks').append(task_html);
<% end %>

$('#complete_tasks').html('');
<%- @complete_tasks.each do |task| %>
var task_html = $("<%= j render('/account/projects/show_tasks', task: task, project: @project) %>");
$('#complete_tasks').append(task_html);
<% end %>
