<% if @invitation.persisted? %>
  $('#workspace-members')
  .append("<%=j render 'account/workspaces/show_user', users: [@invitation.user], workspace: @workspace %>")
  $('#new-member').html("<%=j render 'account/workspaces/new_member_link', workspace: @workspace %>")
<% else %>
  $('#new-member').html("<%= j render 'member_form', workspace: @workspace %>");
<% end %>
